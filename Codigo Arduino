#include <LiquidCrystal.h>
int analogPin = 0;
int val = 0;
int seconds = 0;
int leerDatos= 13;
int mostrarDatos = 8;

LiquidCrystal lcd_1(12, 11, 5, 4, 3, 2);

void setup()
{
  lcd_1.begin(16, 2);

  
  lcd_1.print("Listo");
   Serial.begin(9600);
  pinMode(leerDatos, INPUT);
  pinMode(mostrarDatos,INPUT);
}

void loop()
{
  if(digitalRead(leerDatos)==HIGH)
  {
    val = analogRead(analogPin);
    Serial.println(val);
  }
  
  lcd_1.setCursor(0, 1);
  lcd_1.print(val);
  delay(100); 
  seconds += 1;
  
}
